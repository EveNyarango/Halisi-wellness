<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.halisiwellness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HalisiWellness">
        <activity android:name=".ui.CalenderActivity"></activity>
        <activity android:name=".ui.MeelPlanActivity" />
        <activity android:name=".ui.RegisterActivity" />
        <activity android:name=".ui.OtpActivity" />
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.DiaryActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.halisiwellness.ui.MainActivity" />
        </activity>
        <activity android:name=".ui.ReminderActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.halisiwellness.ui.MainActivity" />
        </activity>
        <activity android:name=".ui.ChatBoxActivity" />
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.ArticlesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.halisiwellness.ui.MainActivity" />
        </activity>
        <activity android:name=".ui.AppointmentActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.halisiwellness.ui.MainActivity" />
        </activity>
        <activity android:name=".ui.DashboardActivity" />
        <activity android:name=".ui.ProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.halisiwellness.ui.MainActivity" />
        </activity>
    </application>

</manifest>